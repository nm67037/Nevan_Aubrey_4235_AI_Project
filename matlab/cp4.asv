data = readtable("rpm_log.txt");
time = data.time;
rpm = data.RPM;

sys = tf(6500,[2000,1]);
[y,t] = step(sys);

figure;
plot(time,rpm);
hold on;
plot(t,y);
yline(6500,'LineWidth',2);
y_line = 0.63*(6500);
yline(y_line, 'LineWidth',2);
xline(2026,'LineWidth',2,'Color','g')
xlabel("time(ms)");
ylabel("RPM");